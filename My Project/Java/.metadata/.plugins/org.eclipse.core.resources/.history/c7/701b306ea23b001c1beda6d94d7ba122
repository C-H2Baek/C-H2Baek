class Solve23{
	
	
	
	public static void main(String[] args) {
		int[] data = {3,2,9,4,7};
		System.out.println(java.util.Arrays.ToString(data));
		System.out.println("최대값 :" + max(data)); 
		System.out.println("최대값 :" + max(null));			 	
		Systme.out.println("최대값 :" + max(new int[] {}));	 // 크기가 0인 배열
	}
	
	
	
	
	
	
	
	
	
	
}
