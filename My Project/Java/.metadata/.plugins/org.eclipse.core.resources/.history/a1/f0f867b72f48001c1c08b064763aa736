class Solve5_1 {
	private int size;
	public Solve5_1(int size) {this.size = size;}
	protected int getSize() { return size; }
}

class ColorTv extends Solve5_1 {
	private int color;
	public ColorTV(int size, int color) {
		super(size);
		this.color = color ;
	}
	protected int getColor() { return color; }
	public void printProperty() {
		System.out.println
	}
}



public static void main(String [] args) {
	ColorTv Solve5_1 = new Color TV(32, 1024);
	Solve5_1.printProperty();
}
