class MyTv {
	boolean isPowerOn;
	int channel;
	int volume;
	
	final int MAX_VOLUME = 100;
	final int MIN_VOLUME = 0;
	final int MAX_CHANNEL = 100;
	final int MN_CHANNEL = 1;
	
	void turnOnOff() {
		
		// (1)is
	}
	
	void volumeUp() {
		
		// (2)
	}
	
	void volumeDown() {
		
		// (3)
	}
	
	void channelUp() {
		
		// (4)
	}
	
	void channelDown() {
		
		// (5)
	}	
}

class Solve21 {
	public static void main(String args[]) {
		MyTv t = new MyTv();
		
		t.channel = 100;
		t.volume = 0;
		System.out.println("CH:"+t.channel+", VOL:" + t.volume);
		
		t.channelDown();
		t.volumeDown();
		System.out.println("CH:"+t.channel+", VOL:" + t.volume);
		
		t.channel = 100;
		t.channelUp();
		t.volumeUp();
		System.out.println("CH:"+t.channel+", VOL:" + t.volume);
	}
}
