import java.util.Scanner;

public class Triangle {
	public static void main (String args[])  {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Insert 3 Length>>");
		int l1 = scanner.nextInt();
		int l2 = scanner.nextInt();
		int l3 = scanner.nextInt();
		if ((l1+12)<l3 || (l1+l3)<l2 ||(l2+l3)<l1) // 두변의 합이 다른 한변보다 작은 경우 ||는 or로 하나라도 만족하면 만들수 없다
			System.out.println("This Length cannot make Triangle.");
				
		else
			System.out.println("This Length can make Trianle");
		
		scanner.close();
				
	}
}
